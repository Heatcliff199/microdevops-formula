{
  "title": "{{ board_name }}",
  "editable": true,
  "panels": [
    {
      "id": 1,
      "collapsed": false,
      "gridPos": {
        "h": 1,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "title": "Disk Space and Memory",
      "type": "row"
    },
    {
      "title": "[{{ node_name }}] Disk Space Usage",
      "id": 2,
      "alert": {
        "name": "[{{ node_name }}] Disk Space Usage > 80%",
        "conditions": [
          {
            "evaluator": {
              "params": [
                0.8
              ],
              "type": "gt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": [
                "A",
                "5m",
                "now"
              ]
            },
            "reducer": {
              "type": "max"
            },
            "type": "query"
          }
        ],
        "executionErrorState": "alerting",
        "for": "0",
        "frequency": "1m",
        "handler": 1,
        "noDataState": "no_data"
      },
      "bars": false,
      "dashLength": 10,
      "dashes": false,
      "fill": 10,
      "fillGradient": 0,
      "gridPos": {
        "h": 6,
        "w": 7,
        "x": 0,
        "y": 1
      },
      "hiddenSeries": false,
      "legend": {
        "avg": false,
        "current": false,
        "max": false,
        "min": false,
        "show": true,
        "total": false,
        "values": false
      },
      "lines": true,
      "linewidth": 1,

      "options": {
        "alertThreshold": true
      },
      "percentage": false,

      "pointradius": 2,
      "points": false,
      "renderer": "flot",
      "spaceLength": 10,
      "stack": false,
      "steppedLine": false,
      "targets": [
        {
          "exemplar": true,
          "expr": "max by (node_name) (1 - (max_over_time(node_filesystem_free_bytes{node_name=~\"{{ node_name }}\", fstype!~\"rootfs|selinuxfs|autofs|rpc_pipefs|tmpfs\"}[5m]) or max_over_time(node_filesystem_free_bytes{node_name=~\"{{ node_name }}\", fstype!~\"rootfs|selinuxfs|autofs|rpc_pipefs|tmpfs\"}[5m])) / \n(max_over_time(node_filesystem_size_bytes{node_name=~\"{{ node_name }}\", fstype!~\"rootfs|selinuxfs|autofs|rpc_pipefs|tmpfs\"}[5m]) or\nmax_over_time(node_filesystem_size_bytes{node_name=~\"{{ node_name }}\", fstype!~\"rootfs|selinuxfs|autofs|rpc_pipefs|tmpfs\"}[5m])))",
          "interval": "",
          "legendFormat": "Disk Space Usage",
          "refId": "A"
        }
      ],
      "thresholds": [
        {
          "colorMode": "critical",
          "fill": true,
          "line": true,
          "op": "gt",
          "value": 0.8,
          "visible": true
        }
      ],
      "tooltip": {
        "shared": true,
        "sort": 0,
        "value_type": "individual"
      },
      "type": "graph",
      "xaxis": {
        "mode": "time",
        "show": true
      },
      "yaxes": [
        {
          "$$hashKey": "object:392",
          "format": "percentunit",
          "logBase": 1,
          "show": true
        },
        {
          "$$hashKey": "object:393",
          "format": "percentunit",
          "logBase": 1,
          "show": false
        }
      ],
      "yaxis": {
        "align": false
      }
    },
    {
      "id": 3,
      "alert": {
        "name": "[{{ node_name }}] Free Memory < 10%",
        "conditions": [
          {
            "evaluator": {
              "params": [
                10
              ],
              "type": "lt"
            },
            "operator": {
              "type": "and"
            },
            "query": {
              "params": [
                "A",
                "1m",
                "now"
              ]
            },
            "reducer": {
              "type": "min"
            },
            "type": "query"
          }
        ],
        "executionErrorState": "alerting",
